{{#hasProtectionClasses}}
<div class="reportSection ">
  <h4>Commercial Fishing</h4>
  <p class="no-data">No data available to report on.<p>

</div>
<div class="reportSection tableContainer">
  <h4>Recreational Fishing</h4>
  <table data-paging="10">
    <thead>
      <tr>
        <th>Name</th>
        <!--
        <th>Affected Area (%)</th>
        <th>Level of Fishing Displaced (%)</th>
        -->
      </tr>
    </thead>
    <tbody>
      {{#protectionRecreationalFishing}}
      <tr>
        <td>{{Name}}</td>
        <!--
        <td>{{PERC_DISP}}</td>
        <td>{{LVL_DISP}}</td>
        -->
      </tr>
      {{/protectionRecreationalFishing}}
    </tbody>

  </table>
</div>

{{/hasProtectionClasses}}

