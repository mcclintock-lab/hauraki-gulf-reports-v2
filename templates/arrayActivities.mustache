{{#sketchClass.deleted}}
<div class="alert alert-warn" style="margin-bottom:10px;">
  This sketch was created using the "{{sketchClass.name}}" template, which is
  no longer available. You will not be able to copy this sketch or make new
  sketches of this type.
</div>
{{/sketchClass.deleted}}

{{#hasProtectionClasses}}
  <div class="reportSection tableContainer">
    <h4>Existing Uses</h4>
      {{#hasProtectionAllExistingUses}}
        <p class="large">
          A sketch in your collection overlaps, or is close to, identified existing uses.
        </p>
        <table data-paging="10"> 
          <thead>
            <tr>
              <th class="single-column">Existing Use</th>
            </tr>
          </thead>
          <tbody>
            {{#protectionAllExistingUses}}
              <tr>
                <td>{{FEAT_TYPE}}</td>
              </tr>
            {{/protectionAllExistingUses}}
          </tbody>
        </table>
    {{/hasProtectionAllExistingUses}}
    {{^hasProtectionAllExistingUses}}
      <p class="large">
        <strong>No</strong> protection classes overlap with existing uses.
      </p>
    {{/hasProtectionAllExistingUses}}
  </div>


<div class="reportSection tableContainer">
  <h4>Heritage Values <a href="#" data-toggle-node="52d5b094c03e5164170003ca" data-visible="false">show layer</a></h4>
    {{#hasProtectionHeritageUses}}
      <p class="large">
        <strong>One or more sketches overlap, or are in the vicinity of, an area that has been identified as having cultural heritage values</strong>.
      </p>
      <table data-paging="10"> 
        <thead>
          <tr>
            <th>Heritage Value</th>
            <th>Number of Sites</th>
          </tr>
        </thead>
        <tbody>
          {{#hasProtectionShipwrecks}}
            <tr>
              <td>Shipwrecks</td>
              <td>{{protectionNumShipwrecks}}</td>
            </tr>
          {{/hasProtectionShipwrecks}}
          {{#hasProtectionHistoricPlaces}}
            <tr>
              <td>Historic Places</td>
              <td>{{protectionNumHistoricPlaces}}</td>
            </tr>
          {{/hasProtectionHistoricPlaces}}
          {{#hasProtectionArcheologicalSites}}
            <tr>
              <td>Archeological Sites</td>
              <td>{{protectionNumArcheologicalSites}}</td>
            </tr>
          {{/hasProtectionArcheologicalSites}}
        </tbody>
      </table>
  {{/hasProtectionHeritageUses}}
  {{^hasProtectionHeritageUses}}
    <p class="large">
      <strong>No</strong> protection classes overlap with areas identified as having cultural heritage values.
    </p>
  {{/hasProtectionHeritageUses}}
</div>

{{#hasProtectionOverlapWithMooringsAndAnchorages}}
  <div class="reportSection">
    <h4>Mooring and Anchorage Areas</h4>
      <p class="large green-check">
        <strong>One more more protection areas overlap with sites that are identified as good for Mooring and Anchorages</strong>.
      </p>
      <p style="margin-left:50px;">
        <a href="#" data-toggle-node="53191b6deb1fe0ac0d0b2031" data-visible="false">Show Coastal Marine Management Areas</a>
      </p>
  </div>
{{/hasProtectionOverlapWithMooringsAndAnchorages}}
{{/hasProtectionClasses}}

{{#hasAquacultureClasses}}
  <div class="reportSection tableContainer">
    <h4>Existing Uses</h4>
      {{#hasAquacultureAllExistingUses}}
        <p class="large">
          <strong>Your sketch overlaps, or is close to, an identified existing use.</strong>
        </p>
        <table data-paging="10">
          <thead>
            <tr>
              <th class="single-column">Existing Use</th>
            </tr>
          </thead>
          <tbody>
            {{#aquacultureAllExistingUses}}
              <tr>
                <td>{{FEAT_TYPE}}</td>
              </tr>
            {{/aquacultureAllExistingUses}}
          </tbody>
        </table>
    {{/hasAquacultureAllExistingUses}}
    {{^hasAquacultureAllExistingUses}}
        <p class="large">
          <strong>No</strong> aquaculture areas overlap with existing uses.
        </p>
    {{/hasAquacultureAllExistingUses}}
  </div>

  <div class="reportSection tableContainer">
    <h4>Heritage Values<a href="#" data-toggle-node="52d5b094c03e5164170003ca" data-visible="false">show layer</a></h4>
      {{#hasAquacultureHeritageUses}}
        <p class="large">
          <strong>One or more sketches overlap, or are in the vicinity of, an area that has been identified as having cultural heritage values</strong>.
        </p>
        <table data-paging="10"> 
          <thead>
            <tr>
              <th>Heritage Value</th>
              <th>Number of Sites</th>
            </tr>
          </thead>
          <tbody>
            {{#hasAquacultureShipwrecks}}
              <tr>
                <td>Shipwrecks</td>
                <td>{{aquacultureNumShipwrecks}}</td>
              </tr>
            {{/hasAquacultureShipwrecks}}
            {{#hasAquacultureHistoricPlaces}}
              <tr>
                <td>Historic Places</td>
                <td>{{aquacultureNumHistoricPlaces}}</td>
              </tr>
            {{/hasAquacultureHistoricPlaces}}
            {{#hasAquacultureArcheologicalSites}}
              <tr>
                <td>Archeological Sites</td>
                <td>{{aquacultureNumArcheologicalSites}}</td>
              </tr>
            {{/hasAquacultureArcheologicalSites}}
          </tbody>
        </table>
        
    {{/hasAquacultureHeritageUses}}
    {{^hasAquacultureHeritageUses}}
      <p class="large">
        <strong>No</strong> aquaculture areas overlap with heritage areas identified as having <strong>cultural heritage values</strong>.
      </p>
    {{/hasAquacultureHeritageUses}}
  </div>
  
{{#hasAquacultureOverlapWithMooringsAndAnchorages}}
  <div class="reportSection">
    <h4>Mooring and Anchorage Areas </h4>
        <p class="large green-check">
          <strong>One more more aquaculture areas overlap with sites that are identified as good for Mooring and Anchorages</strong>.
        </p>
        <p style="margin-left:50px;">
          <a href="#" data-toggle-node="53191b6deb1fe0ac0d0b2031" data-visible="false">Show Coastal Marine Management Areas</a>
        </p>
  </div>
{{/hasAquacultureOverlapWithMooringsAndAnchorages}}
{{/hasAquacultureClasses}}

