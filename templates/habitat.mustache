      {{/sensitiveAreas}}
    </tbody>
  </table>
</div>
{{/hasSensitiveAreas}}

{{#hasProtectedMammals}}
<div class="reportSection tableContainer">
  <h4>Marine Mammals</h4>
  <table data-paging="10">
    <thead>
      <tr>
        <th>Top 10 Species Present</th>
        <th>Threat Status</th>
        <th>Number of Recorded Sightings</th>

      </tr>
    </thead>
    <tbody>
      {{#protectedMammals}}
      <tr>
        <td>{{Name}}</td>
        <td>--</td>
        <td>{{Count}}</td>
      </tr>
      {{/protectedMammals}}
    </tbody>
    <tfoot>
      <tr>
        <td colspan="3" style="text-align:left;">
          <p class="large">
            The Threat Status refers to the New Zealand Threat Classification System. <a href="http://www.doc.govt.nz/publications/conservation/nz-threat-classification-system/" target="_blank">Read more about the classification system here.</a>
          </p>
        </td>
      </tr>
    </tfoot>
  </table>
</div>
{{/hasProtectedMammals}}

