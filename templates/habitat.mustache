{{#hasRepresentationData}}
  <div class="reportSection tableContainer">
    <h4>Habitat Representation</h4>
    {{#d3IsPresent}}
      <table data-paging="10" class="hab_rep_table">
        <thead>
          <tr>
            <th class="sorting_col" style="width:145px;"><a class="hab_rep_type sort_up" href="#">Habitats</a></th>
            <th style="width:75px;"><a  class="hab_rep_perc sort_down" href="#" >% of each Habitat Protected</a></th>
            <th style="width:90px;"><a class="hab_rep_num_reserves sort_down" href="#"># of Sites Protected in Marine Reserves</a></th>
            <th style="width:90px;"><a class="hab_rep_num_type2 sort_down"  href="#"># of Sites Protected in Type-2 Protected Areas</a></th>
          </tr>
        </thead>
        <tbody class="hab_rep_values">
        </tbody>
      </table>
    {{/d3IsPresent}}
    {{^d3IsPresent}}
      <table data-paging="10">
        <thead>
          <tr>
            <th>Habitats</th>
            <th>% of each Habitat Protected</th>
            <th>Number of Sites Protected in Marine Reserves</th>
            <th>Number of Sites Protected in Type-2 Protected Areas</th>
          </tr>
        </thead>
        <tbody>
          {{#representationData}}
          <tr>
            <td>{{HAB_TYPE}}</td>
            <td>{{CB_PERC}}</td>
            <td>{{REP_COUNT}}</td>
            <td>{{NEW_SIZE}}</td>
          </tr>
          {{/representationData}}
        </tbody>
      </table>
    {{/d3IsPresent}}
  </div>
{{/hasRepresentationData}}

