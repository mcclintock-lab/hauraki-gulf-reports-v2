{{#sketchClass.deleted}}
<div class="alert alert-warn" style="margin-bottom:10px;">
  This sketch was created using the "{{sketchClass.name}}" template, which is
  no longer available. You will not be able to copy this sketch or make new
  sketches of this type.
</div>
{{/sketchClass.deleted}}

{{#isCloseToProtectedAreas}}
<div class="reportSection tableContainer">
  <h4>Proximity to Existing Protected Areas</h4>
  <table>
    <thead>
      <tr>
        <th>Aquaculture Zone Name</th>
        <th>Type of Protected Area Impacted</th>
        <th>Likelihood of Impact</th>
      </tr>
    </thead>
    <tbody>
      {{#proximityToProtectedAreas}}
        <tr>
          <td>{{ZoneName}}</td>
          <td>{{NEAR_TYPE}}</td>
          <td>{{THRESHOLD}}</td>
        </tr>
      {{/proximityToProtectedAreas}}
    </tbody>
  </table>
</div>
{{/isCloseToProtectedAreas}}
{{^isCloseToProtectedAreas}}
<div class="reportSection">
  <h4>Proximity to Existing Protected Areas</h4>
  <p class="large">
    This zone is <strong>not</strong> within 1 kilometer of any existing protected areas.
  </p>
</div>
{{/isCloseToProtectedAreas}}

<div class="reportSection tableContainer">
  <h4>Habitats in Aquaculture Areas</h4>
  {{#d3IsPresent}}
      <table data-paging="10" class="aquaculture_hab_table"> 
      <thead>
        <tr>
          <th class="sorting_col" style="width:200px;"><a class="hab_aquaculture_type sort_up" href="#">Habitat Type</a></th>
          <th><a class="hab_aquaculture_new sort_down" href="#">Area of Habitat Present (ha)</a></th>
          <th><a class="hab_aquaculture_new_perc sort_down" href="#">% of Habitat in the Gulf</a></th>
        </tr>
      </thead>
      <tbody class="aquaculture_values"></tbody>
    </table>
  {{/d3IsPresent}}
  {{^d3IsPresent}}
    <table data-paging="10">
      <thead>
        <tr>
          <th style="width:200px;">Habitat Type</th>
          <th>Area of Habitat Present (ha)</th>
          <th>% of Habitat in the Gulf</th>
        </tr>
      </thead>
      <tbody>
        {{#aquacultureHabitats}}
        <tr>
          <td>{{HAB_TYPE}}</td>    
          <td>{{NEW_SIZE}}</td>
          <td>{{NEW_PERC}}</td>
        </tr>
        {{/aquacultureHabitats}}
      </tbody>
      <tfoot>
        <p>
          <strong>{{habitatsInAquacultureZones}}</strong> of <strong>{{habitatsCount}}</strong> habitats
          are located inside new and existing aquaculture areas.
        </p>
      </tfoot>
    </table>
  {{/d3IsPresent}}
</div>

{{#hasSensitiveAreas}}
<div class="reportSection tableContainer">
  <h4>Sensitive Habitats</h4>
  <table data-paging="10">
    <thead>
      <tr>
        <th>Name</th>
        <th>Type</th>
        <th>Hectares Protected</th>
        <th>Percent of Area Protected</th>
      </tr>
    </thead>
    <tbody>
      {{#sensitiveAreas}}
      <tr>
        <td>{{SA_NAME}}</td>
        <td>{{SA_TYPE}}</td>
        <td>{{CLPD_AREA}}</td>
        <td>{{PERC_AREA}}</td>
      </tr>
      {{/sensitiveAreas}}
    </tbody>
  </table>
</div>
{{/hasSensitiveAreas}}

