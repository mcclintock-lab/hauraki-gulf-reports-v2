          <td>{{Class}}</td>
          <td>{{AreaInHa}}</td>
          <td>{{Percent}}</td>
        </tr>
        {{/aquacultureEcosystemProductivity}}
      </tbody>
    </table>
    </div>
  </div>
  <div style="height:50px;"></div>

