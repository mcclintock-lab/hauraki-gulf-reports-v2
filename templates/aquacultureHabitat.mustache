{{#sketchClass.deleted}}
<div class="alert alert-warn" style="margin-bottom:10px;">
  This sketch was created using the "{{sketchClass.name}}" template, which is
  no longer available. You will not be able to copy this sketch or make new
  sketches of this type.
</div>
{{/sketchClass.deleted}}

{{#isCloseToProtectedAreas}}
<div class="reportSection tableContainer">
  <h4>Proximity to Existing Protected Areas</h4>
  <table>
    <thead>
      <tr>
        <th>Aquaculture Zone Name</th>
        <th>Type of Protected Area Impacted</th>
        <th>Likelihood of Impact</th>
      </tr>
    </thead>
    <tbody>
      {{#proximityToProtectedAreas}}
        <tr>
          <td>{{ZoneName}}</td>
          <td>{{NEAR_TYPE}}</td>
          <td>{{THRESHOLD}}</td>
        </tr>
      {{/proximityToProtectedAreas}}
    </tbody>
  </table>
</div>
{{/isCloseToProtectedAreas}}

{{^isCloseToProtectedAreas}}
<div class="reportSection">
  <h4>Proximity to Existing Protected Areas</h4>
  <p class="large">
    This zone is <strong>not</strong> within 1 kilometer of any existing protected areas.
  </p>
</div>
{{/isCloseToProtectedAreas}}


{{#hasSensitiveAreas}}
<div class="reportSection tableContainer">
  <h4>Sensitive Habitats</h4>
  <table data-paging="10">
    <thead>
      <tr>
        <th>Name</th>
        <th>Type</th>
        <th>Hectares Protected</th>
        <th>Percent of Area Protected</th>
      </tr>
    </thead>
    <tbody>
      {{#sensitiveAreas}}
      <tr>
        <td>{{SA_NAME}}</td>
        <td>{{SA_TYPE}}</td>
        <td>{{CLPD_AREA}}</td>
        <td>{{PERC_AREA}}</td>
      </tr>
      {{/sensitiveAreas}}
    </tbody>
  </table>
</div>
{{/hasSensitiveAreas}}

{{#hasProtectedMammals}}
<div class="reportSection tableContainer">
  <h4>Protected Mammals</h4>
  <table data-paging="10">
    <thead>
      <tr>
        <th>Name</th>
        <th>Status</th>
        <th>Number of Protected Mammal Areas</th>

      </tr>
    </thead>
    <tbody>
      {{#protectedMammals}}
      <tr>
        <td>{{Name}}</td>
        <td>--</td>
        <td>{{Count}}</td>
      </tr>
      {{/protectedMammals}}
    </tbody>
  </table>
</div>
{{/hasProtectedMammals}}

{{#hasSeabirdBreedingSites}}
<div class="reportSection tableContainer">

  <h4>Seabirds</h4>
  <table data-paging="10">
    <thead>
      <tr>
        <th>Name</th>

        <th>Status</th>

        <th>Number of Breeding Sites</th>
      </tr>
    </thead>
    <tbody>
      {{#seabirdBreedingSites}}
      <tr>
        <td>{{Name}}</td>
        <td>--</td>
        <td>{{Count}}</td>
      </tr>
      {{/seabirdBreedingSites}}
    </tbody>
  </table>
</div>
{{/hasSeabirdBreedingSites}}


{{#hasShorebirdSites}}
<div class="reportSection tableContainer">
  <h4>Shorebird Sites</h4>
  <table data-paging="10">
    <thead>
      <tr>
        <th>Name</th>
        <th>Status</th>
        <th>Number of Shorebird Sites</th>
      </tr>
    </thead>
    <tbody>
