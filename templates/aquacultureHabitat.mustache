{{^isCollection}}
{{#adjacentProtectedAreas}}
<div class="reportSection">
  <h4>Adjacent Terrestrial Protected Area</h4>
  <p class="large green-check">
    This zone is adjacent to a <strong>Terrestrial Protected Area</strong>.
  </p>
</div>
{{/adjacentProtectedAreas}}
{{/isCollection}}

  <div class="reportSection" class="tableContainer">
    <h4>Ecosystem Services</h4>
      <p>
        Percent overlap of the proposed aquaculture areas for three types of ecosystem services.
      </p>
      <p>
        <select class="aquaculture-chosen">
          {{#aquacultureEcosystemServices}}
          <option value="{{.}}">{{.}}</option>
          {{/aquacultureEcosystemServices}}
        </select>
      </p>
    <div class="aquaculture-nutrient-recycling">
      <table>
        <thead>
          <tr>
            <th style="width:200px;">Nutrient Recycling Value</th>
            <th>Area (Ha)</th>
            <th>Percent</th>
          </tr>
        </thead>
        <tbody>
        {{#aquacultureNutrientRecycling}}
          <tr>
            <td>{{Class}}</td>
            <td>{{AreaInHa}}</td>
            <td>{{Percent}}</td>
          </tr>
          {{/aquacultureNutrientRecycling}}
        </tbody>
      </table>
    </div>
    <div class="aquaculture-biogenic-habitat">
      <table>
        <thead>
          <tr>
            <th style="width:200px;">Biogenic Habitat Value</th>
            <th>Area (Ha)</th>
            <th>Percent</th>
          </tr>
        </thead>
        <tbody>
          {{#aquacultureBiogenicHabitat}}
          <tr>
            <td>{{Class}}</td>
            <td>{{AreaInHa}}</td>
            <td>{{Percent}}</td>
          </tr>
          {{/aquacultureBiogenicHabitat}}
        </tbody>
      </table>
    </div>
    <div class="aquaculture-ecosystem-productivity">
    <table>
      <thead>
        <tr>
          <th style="width:200px;">Ecosystem Productivity Value</th>
          <th>Area (Ha)</th>
          <th>Percent</th>
        </tr>
      </thead>
      <tbody>
      {{#aquacultureEcosystemProductivity}}
        <tr>
          <td>{{Class}}</td>
          <td>{{AreaInHa}}</td>
          <td>{{Percent}}</td>
        </tr>
        {{/aquacultureEcosystemProductivity}}
      </tbody>
    </table>
    </div>
  </div>
